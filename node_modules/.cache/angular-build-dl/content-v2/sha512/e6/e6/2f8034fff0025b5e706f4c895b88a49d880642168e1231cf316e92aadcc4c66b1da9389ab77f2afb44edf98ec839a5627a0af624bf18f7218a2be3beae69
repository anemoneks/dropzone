HttpClientHouseService.deleteHouse(x._id);\r\n    }) || [];\r\n\r\n    forkJoin(deleted)\r\n      .pipe(defaultIfEmpty(null))\r\n      .subscribe(x => {\r\n        this.houses = this.houses.filter(y => {\r\n          var found = this.selection.selected.filter(z => {\r\n            return z._id == y._id;\r\n          }) || [];\r\n          return found.length <= 0;\r\n        });\r\n\r\n        this.rerender();\r\n        this.selection.clear();\r\n      });\r\n  }\r\n}","<!-- ============================================================== -->\r\n<!-- Table -->\r\n<!-- ============================================================== -->\r\n<div class=\"row\">\r\n    <!-- column -->\r\n    <div class=\"col-12\">\r\n        <div class=\"card\">\r\n            <div class=\"card-body\">\r\n                <table datatable [dtOptions]=\"dtOptions\" [dtTrigger]=\"dtTrigger\" class=\"table table-striped table-bordered table-sm row-border hover\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th>Unit</th>\r\n                            <th>Street</th>\r\n                            <th>&nbsp;</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr *ngFor=\"let house of houses\">\r\n                            <td><a [routerLink]=\"['/component/administration/houses/edit/' + house._id]\">{{ house.unit }}</a></td>\r\n                            <td><a [routerLink]=\"['/component/administration/houses/edit/' + house._id]\">{{ house.street }}</a></td>\r\n                            <td>\r\n                                <div class=\"pretty p-icon\">\r\n                                    <input class=\"form-check-input\" type=\"checkbox\" id=\"gridCheck\"\r\n                                        (click)=\"$event.stopPropagation()\"\r\n                                        (change)=\"$event ? selection.toggle(house) : null\"\r\n                           